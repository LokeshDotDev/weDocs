{
	"CORSRules": [
		{
			"AllowedOrigins": [
				"http://localhost:3000",
				"http://localhost:3001",
				"http://localhost:8080",
				"http://host.docker.internal:3000",
				"http://host.docker.internal:3001",
				"http://host.docker.internal:8080"
			],
			"AllowedMethods": ["GET", "HEAD", "PUT", "POST", "DELETE", "OPTIONS"],
			"AllowedHeaders": ["*"],
			"ExposeHeaders": ["ETag", "x-amz-meta-*"],
			"MaxAgeSeconds": 3000
		}
	]
}
